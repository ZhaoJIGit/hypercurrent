    <add key="DefaultHeadImage" value="D:\MegiIIS\my.megichina.com\Images\default-header.jpg"/>
    <!--是否记录异常日志-->
    <add key="writeLog" value="1"/>
    <!--日志文件大小 KB-->
    <add key="logFileSize" value="1024"/>
    <!--日志文件记录的位置-->
    <add key="logFilePath" value="d:\megiiis\my.megichina.com\log\"/>
