@{
    ViewBag.Title = @HtmlLang.Write(LangModule.Login, "RegisterSuccess", "Register Success");
}
@using JieNor.Megi.Common.Utility
<style>
    body { overflow: hidden; }
</style>

<div class="m-reg-box">
    <div id="loginBox" class="m-login-box">
        <div class="container form-login-content main-reg  m-send-success">
            <h1>&nbsp;</h1>
            <div class="space"></div>
            <div class="space"></div>
            <div class="space"></div>
            <div>
                <div class="m-login-box-title">@HtmlLang.Write(LangModule.Login, "CheckYourInbox", "Check your inbox")</div>
                <div class="m-login-box-title-tip">@HtmlLang.Write(LangModule.Login, "WeSendYouAnEmail", "We’ll send you an email shortly with a link to verify your account.")</div>

            </div>

            <div class="empty-content"></div>
            <div class="m-signIn-wrap">
                <div class="wrap-problem">@HtmlLang.WriteFormat(LangModule.Login, "HaveProblem", "Have problems? Please <a href=\"mailto:{0}\">Contact us</a> and we will work it out!", System.Configuration.ConfigurationManager.AppSettings["ServiceEmail"]) </div>
                <div class="wrap-have-account">
                    <span>@HtmlLang.Write(LangModule.Login, "AlreadyHaveAnAccount", "Already have an account?")</span>
                </div>
                <div class="wrap-btn">
                    <a id="aSignInHere" href="@ServerHelper.LoginServer">@HtmlLang.Write(LangModule.Login, "SignInHere", "Sign in here")</a>
                </div>
            </div>
        </div>
    </div>
</div>




<script src="@ServerHelper.StaticServer/FW/scripts/jquery.megi.fw.reg.js?@ServerHelper.JSVersion"></script>
<script src="@ServerHelper.ScriptServer/main/scripts/User/Register.js?@ServerHelper.JSVersion"></script>
