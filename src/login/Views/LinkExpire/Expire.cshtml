@{
    ViewBag.Title = "Expire";
    Layout = "~/Views/Shared/_ForgetPasswordLayout.cshtml";
}


<div class="m-reg-box">
    <div id="loginBox" class="m-login-box m-fail-img">
        <div class="container form-login-content main-reg" >
            <h1>&nbsp;</h1>
            <div class="space"></div>
            <div class="space"></div>
            <div class="space"></div>
            <div>
                <div class="m-login-box-title">@HtmlLang.Write(LangModule.Login, "SorryThatLinkHasExpiredTitle", "Link Expired!")</div>
                <div class="m-login-box-title-tip"> 
                    @HtmlLang.WriteFormat(LangModule.Login, "SorryThatLinkHasExpired", "Sorry that link has expired. please click <a href='{0}'>Here</a> to operate again!","/")
</div>
            </div>

            <div class="empty-content"></div>
            <div class="m-signIn-wrap">
                <div class="wrap-problem">@HtmlLang.WriteFormat(LangModule.Login, "HaveProblem", "Have problems? Please <a href=\"mailto:{0}\">Contact us</a> and we will work it out!", System.Configuration.ConfigurationManager.AppSettings["ServiceEmail"]) </div>
                <div class="wrap-have-account">
                    <span>@HtmlLang.Write(LangModule.Login, "AlreadyHaveAnAccount", "Already have an account?")</span>
                </div>
                <div class="wrap-btn">
                    <a id="aSignInHere" href="@ServerHelper.LoginServer">@HtmlLang.Write(LangModule.Login, "SignInHere", "Sign in here")</a>
                </div>
            </div>

        </div>
    </div>
</div>

